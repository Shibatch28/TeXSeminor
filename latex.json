      "% ドキュメントクラス =================",
      "\\documentclass[12pt, a4paper]{ltjsarticle}",
      "% ====================================",
      "",
      "% 読み込むパッケージ =================",
      "\\usepackage[dvipsnames]{xcolor}",
      "\\usepackage[luatex]{graphicx}",
      "\\usepackage{ascmac}",
      "\\usepackage{CJKutf8}",
      "\\usepackage{luatex85}",
      "\\usepackage{multicol}",
      "\\usepackage{physics, amssymb, amsmath}",
      "\\usepackage{enumitem}",
      "\\usepackage{here}",
      "\\usepackage{listings, jvlisting}",
      "\\usepackage{tikz, tikz-3dplot, braket}",
      "\\usepackage{subcaption}",
      "% =====================================",
      "",
      "% TikZの設定===========================",
      "\\usetikzlibrary{shapes, shapes.geometric, shapes.misc, positioning, through}",
      "\\usetikzlibrary{patterns, intersections, calc}",
      "\\usetikzlibrary{quotes, angles, arrows.meta}",
      "% =====================================",
      "",
      "% 余白の設定 ==========================",
      "\\setlength{\\textheight}{\\paperheight}",
      "\\setlength{\\topmargin}{4.6truemm}",
      "\\addtolength{\\topmargin}{-\\headheight}",
      "\\addtolength{\\topmargin}{-\\headsep}",
      "\\addtolength{\\textheight}{-60truemm}",
      "",
      "\\setlength{\\textwidth}{\\paperwidth}",
      "\\setlength{\\oddsidemargin}{4.6truemm}",
      "\\setlength{\\evensidemargin}{\\oddsidemargin}",
      "\\addtolength{\\textwidth}{-61truemm}",
      "% =====================================",
